    "@tailwindcss/vite": "^4.1.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.14.1",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^4.1.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^5.4.18"
  },
  "trustedDependencies": [
